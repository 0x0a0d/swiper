    "less": "^4.1.2",
    "lint-staged": "^13.0.0",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.14",
    "prettier": "^2.6.2",
    "prettier-plugin-svelte": "^2.7.0",
    "protractor": "~7.0.0",
    "react": "^18.1.0",
    "react-dom": "^18.1.0",
    "rollup": "^2.75.6",
    "rollup-plugin-svelte": "^7.1.0",
    "rxjs": "~7.5.5",
    "solid-js": "^1.4.3",
    "svelte": "^3.48.0",
    "terser": "^5.14.0",
    "ts-node": "~10.8.1",
    "tslib": "^2.4.0",
    "tslint": "~6.1.0",
    "typescript": "^4.7.3",
    "vite": "^2.9.10",
    "vue": "^3.2.37",
    "zone.js": "~0.11.5"
  },
  "dependencies": {
    "dom7": "^4.0.4",
    "ssr-window": "^4.0.2"
  },
  "lint-staged": {
    "*.{js,svelte,jsx}": "eslint --cache --fix"
  }
}
